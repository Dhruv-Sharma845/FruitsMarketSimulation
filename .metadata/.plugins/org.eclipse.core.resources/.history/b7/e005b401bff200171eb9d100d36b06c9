public class TestMarketSimulation {
	
	public static void main(String[] args)
	{
		int arrivalType = ThreadLocalRandom.current().nextInt(0, 2);
		
		if(arrivalType == 0)
		{
			//code for farmer
			System.out.println("Farmer has come to sell.");
			
		}
		else if(arrivalType == 1)
		{
			//code for consumer
		}
	}
}
